{
    "name": "ToDo",
    "description": "Thunderbird Add-On f√ºr das (experimentelle) SIN ToDo Feature",
    "version": "1.0",
    "applications": {
      "gecko": {
        "id": "toDo@sonn.at",
        "strict_min_version": "78.0"
      }
    },
    "permissions": [
      "messagesRead",
      "accountsRead",
      "messagesMove",
      "activeTab",
      "tabs",
      "notifications"
    ],
    "background": {
      "scripts": [
        "background.js"
      ]
    },
  "message_display_action": {
    "default_title": "Todo add",
    "default_icon": "logo.svg",
    "default_label": "ToDo"
  },
    "experiment_apis": {
      "ImapTools": {
        "schema": "api/imapTools.json",
        "parent": {
          "scopes": ["addon_parent"],
          "script": "api/imapTools.js",
          "paths": [["ImapTools"]]
        }
      },
      "LegacyPrefs": {
        "schema": "api/legacyPrefs.json",
        "parent": {
          "scopes": ["addon_parent"],
          "paths": [["LegacyPrefs"]],
          "script": "api/legacyPrefs.js"
        }
      }
    },
    "manifest_version": 2,
  "icons": {
    "16": "logo.svg",
    "32": "logo.svg",
    "48": "logo.svg",
    "96": "logo.svg"
  }
}