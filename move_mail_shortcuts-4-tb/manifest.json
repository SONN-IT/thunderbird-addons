{
  "name": "Move Mail Shortcuts",
  "description": "Allows to assign keyboard shortcuts for moving mails to folders.",
  "version": "4-SP-1.1",
  "applications": {
    "gecko": {
      "id": "mailMoveShortcuts@extension",
      "strict_min_version": "78.0",
      "update_url": "https://demo.sonn.intern/update.json"
    }
  },
  "homepage_url": "https://github.com/robert7k/moveMailShortcuts",
  "icons": {
	"48": "img/48.png"
  },
  "permissions": [
    "messagesMove",
    "messagesRead",
    "accountsRead",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "commands": {
    "move_mail_0": {
      "description": "Folder 1"
    },
    "move_mail_1": {
      "description": "Folder 2"
    },
    "move_mail_2": {
      "description": "Folder 3"
    },
    "move_mail_3": {
      "description": "Folder 4"
    },
    "move_mail_4": {
      "description": "Folder 5"
    },
    "move_mail_5": {
      "description": "Folder 6"
    },
    "move_mail_6": {
      "description": "Folder 7"
    },
    "move_mail_7": {
      "description": "Folder 8"
    },
    "move_mail_8": {
      "description": "Folder 9"
    },
    "move_mail_9": {
      "description": "Folder 10"
	}
  },
  "options_ui": {
    "page": "options.html"
  },
  "manifest_version": 2
}
