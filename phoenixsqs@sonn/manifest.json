{
    "name": "SIN Phoenix Quicksearch",
    "description": "Schnellsuche auf Grundlage des aktuellen E-Mails",
    "version": "2.0-rc.0",
    "applications": {
      "gecko": {
        "id": "phoenixqs@sonn",
        "strict_min_version": "78.0"
      }
    },
    "permissions": [
      "messagesRead",
      "notifications"
    ],
    "background": {
      "scripts": [
        "background.js"
      ]
    },
    "message_display_action": {
      "default_title": "Suche in SIN mit Betreff oder Absender",
      "default_icon": "logo.svg",
      "default_label": "In SIN Ã¶ffen"
    },
    "commands": {
      "phoenixqs_SendToSIN": {
        "suggested_key": {
          "default": "Ctrl+Alt+S"
        },
        "description": "Send a 'phoenixqs_SendToSIN' event"
      }
    },
    "manifest_version": 2
}