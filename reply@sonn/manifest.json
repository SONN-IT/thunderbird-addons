{
    "name": "Reply-SONN",
    "description": "Automatisches Hinzufügen der @ablage Adresse",
    "version": "2.0-rc.1",
    "applications": {
      "gecko": {
        "id": "reply@sonn.at",
        "strict_min_version": "78.0"
      }
    },
    "permissions": [
      "compose"
    ],
    "background": {
      "scripts": [
        "background.js"
      ]
    },
    "compose_action": {
      "default_title": "Automatisches Hinzufügen der @ablage Adresse",
      "default_icon": "logo.svg",
      "default_label": "@ablage"
    },
    "manifest_version": 2,
  "icons": {
    "16": "logo.svg",
    "32": "logo.svg",
    "48": "logo.svg",
    "96": "logo.svg"
  }
}